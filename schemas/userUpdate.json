{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "newPassword": {
            "type": "string",
            "minLength": 5,
            "maxLength": 20
        },
        "oldPassword": {
            "type": "string",
            "minLength": 5,
            "maxLength": 20
        },
        "userName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "email": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60,
            "format": "email"
        },
        "isAdmin": {
            "type": "boolean"
        },
        "wantsNutritionData": {
            "type": "boolean"
        },
        "privacySetting": {
            "type": "boolean"
        },
        "disabled": {
            "type": "boolean"
        },
        "userUuId": {
            "type": "string"
        },
        "additionalProperties": false,
        "required": ["userUuId", "oldPassword"]
    }
}